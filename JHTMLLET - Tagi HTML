#include <iostream>
#include <string>

using namespace std;

int main()
{
    string lan;
    while(getline(cin,napis))
    {
        for (int i=0; i<lan.length(); i++)
        {
            if (lan[i] == '<')
            {
                while(lan[i] != '>')
                {
                    i++;
                    lan[i] = toupper(lan[i]);
                }
            }
        }
    cout<<lan<<endl;
    }
}
