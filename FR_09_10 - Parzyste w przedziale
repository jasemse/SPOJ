#include <iostream>
using namespace std;

int main() {
	int testy;
	cin>>testy;
	while (testy--)
	{
		int czySaParzyste=0;
		int dolnyPrzedzial, gornyPrzedzial;
		cin>>dolnyPrzedzial>>gornyPrzedzial;
		for (int i=dolnyPrzedzial+1;i<gornyPrzedzial;i++)
		{
			if (i%2==0)
			{
				cout<<i<<" ";
				czySaParzyste++;
			}
		}
		if (czySaParzyste==0) cout<<"BRAK"<<endl;
		else cout<<endl;
	}
	return 0;
}
